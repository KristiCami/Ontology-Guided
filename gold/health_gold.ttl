@prefix health: <http://example.org/health#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

health:Patient a owl:Class .
health:Clinician a owl:Class .
health:Doctor a owl:Class ; rdfs:subClassOf health:Clinician .
health:Nurse a owl:Class ; rdfs:subClassOf health:Clinician .
health:Clinic a owl:Class .
health:Room a owl:Class .
health:Appointment a owl:Class .
health:Visit a owl:Class .
health:Procedure a owl:Class .
health:Diagnosis a owl:Class .
health:Prescription a owl:Class .
health:Medication a owl:Class .
health:LabOrder a owl:Class .
health:LabResult a owl:Class .
health:InsurancePolicy a owl:Class .
health:BillingRecord a owl:Class .
health:Consent a owl:Class .
health:VitalSignMeasurement a owl:Class .

health:appointmentPatient a owl:ObjectProperty ; rdfs:range health:Patient .
health:scheduledWith a owl:ObjectProperty ; rdfs:range health:Doctor .
health:scheduledAt a owl:ObjectProperty ; rdfs:range health:Clinic .
health:visitPerformedBy a owl:ObjectProperty ; rdfs:range health:Clinician .
health:visitAt a owl:ObjectProperty ; rdfs:range health:Clinic .
health:visitRoom a owl:ObjectProperty ; rdfs:range health:Room .
health:includesProcedure a owl:ObjectProperty ; rdfs:range health:Procedure .
health:hasDiagnosis a owl:ObjectProperty ; rdfs:range health:Diagnosis .
health:prescriptionMedication a owl:ObjectProperty ; rdfs:range health:Medication .
health:prescribedTo a owl:ObjectProperty ; rdfs:range health:Patient .
health:hasLabOrder a owl:ObjectProperty ; rdfs:range health:LabOrder .
health:labOrderPatient a owl:ObjectProperty ; rdfs:range health:Patient .
health:labHasResult a owl:ObjectProperty ; rdfs:range health:LabResult .
health:hasInsurance a owl:ObjectProperty ; rdfs:range health:InsurancePolicy .
health:billingFor a owl:ObjectProperty ; rdfs:range health:Visit .
health:consentBy a owl:ObjectProperty ; rdfs:range health:Patient .
health:consentTo a owl:ObjectProperty ; rdfs:range health:Clinician .
health:vitalMeasurementOf a owl:ObjectProperty ; rdfs:range health:Patient .
health:measuredBy a owl:ObjectProperty ; rdfs:range health:Clinician .

health:scheduledStart a owl:DatatypeProperty ; rdfs:range xsd:dateTime .
health:durationMinutes a owl:DatatypeProperty ; rdfs:range xsd:integer .
health:appointmentReason a owl:DatatypeProperty ; rdfs:range xsd:string .
health:diagnosisCode a owl:DatatypeProperty ; rdfs:range xsd:string .
health:procedureCode a owl:DatatypeProperty ; rdfs:range xsd:string .
health:procedureStatus a owl:DatatypeProperty ; rdfs:range xsd:string .
health:dosageText a owl:DatatypeProperty ; rdfs:range xsd:string .
health:frequencyText a owl:DatatypeProperty ; rdfs:range xsd:string .
health:medicationName a owl:DatatypeProperty ; rdfs:range xsd:string .
health:medicationForm a owl:DatatypeProperty ; rdfs:range xsd:string .
health:medicationStrength a owl:DatatypeProperty ; rdfs:range xsd:string .
health:resultValue a owl:DatatypeProperty ; rdfs:range xsd:string .
health:resultDate a owl:DatatypeProperty ; rdfs:range xsd:dateTime .
health:policyNumber a owl:DatatypeProperty , owl:FunctionalProperty ; rdfs:range xsd:string .
health:payer a owl:DatatypeProperty ; rdfs:range xsd:string .
health:billingAmount a owl:DatatypeProperty ; rdfs:range xsd:decimal .
health:billingStatus a owl:DatatypeProperty ; rdfs:range xsd:string .
health:consentDate a owl:DatatypeProperty ; rdfs:range xsd:date .
health:consentScope a owl:DatatypeProperty ; rdfs:range xsd:string .
health:measurementTime a owl:DatatypeProperty ; rdfs:range xsd:dateTime .
health:heartRate a owl:DatatypeProperty ; rdfs:range xsd:integer .
health:systolicBP a owl:DatatypeProperty ; rdfs:range xsd:integer .
health:contactEmail a owl:DatatypeProperty ; rdfs:range xsd:string .
health:contactPhone a owl:DatatypeProperty ; rdfs:range xsd:string .
health:licenseId a owl:DatatypeProperty ; rdfs:range xsd:string .

health:Appointment rdfs:subClassOf [ a owl:Restriction ; owl:onProperty health:appointmentPatient ; owl:someValuesFrom health:Patient ],
    [ a owl:Restriction ; owl:onProperty health:scheduledWith ; owl:someValuesFrom health:Doctor ],
    [ a owl:Restriction ; owl:onProperty health:scheduledStart ; owl:someValuesFrom xsd:dateTime ],
    [ a owl:Restriction ; owl:onProperty health:durationMinutes ; owl:someValuesFrom xsd:integer ].

health:Visit rdfs:subClassOf [ a owl:Restriction ; owl:onProperty health:visitPerformedBy ; owl:someValuesFrom health:Clinician ],
    [ a owl:Restriction ; owl:onProperty health:visitAt ; owl:someValuesFrom health:Clinic ],
    [ a owl:Restriction ; owl:onProperty health:hasDiagnosis ; owl:someValuesFrom health:Diagnosis ].

health:InsurancePolicy owl:hasKey ( health:policyNumber ) .
