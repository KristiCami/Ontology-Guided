@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix atm: <http://example.com/atm#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:TransactionShape
    a sh:NodeShape ;
    sh:targetClass atm:Transaction ;
    sh:property [
        sh:path atm:actor ;
        sh:minCount 1 ;
    ] .
