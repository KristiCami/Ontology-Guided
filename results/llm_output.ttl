@prefix atm: <http://www.example.com/atm#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

atm:ATM rdf:type rdfs:Class .

atm:Transaction rdf:type rdfs:Class .

atm:CardInsertion rdf:type rdfs:Class .

atm:logs rdf:type rdf:Property ;
  rdfs:domain atm:ATM ;
  rdfs:range atm:Transaction .

atm:after rdf:type rdf:Property ;
  rdfs:domain atm:CardInsertion ;
  rdfs:range atm:Transaction .

atm:CardInsertion rdfs:subClassOf [ rdf:type rdfs:Restriction ;
  rdfs:onProperty atm:after ;
  rdfs:allValuesFrom atm:Transaction ] .

atm:ATM rdfs:subClassOf [ rdf:type rdfs:Restriction ;
  rdfs:onProperty atm:logs ;
  rdfs:allValuesFrom atm:Transaction ] .