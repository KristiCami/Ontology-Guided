:Authorization a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasError ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( :BadPassword :BadBankCode :BadAccount ) ] ] ;
    owl:equivalentClass [ a owl:Restriction ;
