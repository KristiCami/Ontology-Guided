@prefix atm: <http://example.com/atm#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

atm:ATM a rdfs:Class ;
    rdfs:subClassOf [ a rdfs:Restriction ;
            rdfs:allValuesFrom atm:Transaction ;
            rdfs:onProperty atm:logs ] .

atm:CardInsertion a rdfs:Class ;
    rdfs:subClassOf [ a rdfs:Restriction ;
            rdfs:allValuesFrom atm:Transaction ;
            rdfs:onProperty atm:after ] .

atm:Transaction a rdfs:Class .

atm:after a rdf:Property ;
    rdfs:domain atm:CardInsertion ;
    rdfs:range atm:Transaction .

atm:logs a rdf:Property ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

