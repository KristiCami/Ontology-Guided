@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom owl:Nothing ;
            owl:onProperty atm:operatedBy ] .

atm:IfMoneyDispensedAmountLogged a owl:Axiom ;
    owl:annotatedProperty rdfs:comment ;
    owl:annotatedSource atm:Withdrawal ;
    owl:annotatedTarget "If the money is dispensed, the amount is logged."^^xsd:string .

atm:IfTransactionSucceedsATMDispensesMoney a owl:Axiom ;
    owl:annotatedProperty rdfs:comment ;
    owl:annotatedSource atm:ATM ;
    owl:annotatedTarget "If the transaction succeeds, the ATM dispenses money."^^xsd:string .

atm:InitiatedIfWithdrawalSequenceSuccessful a owl:Axiom ;
    owl:annotatedProperty rdfs:comment ;
    owl:annotatedSource atm:ATM ;
    owl:annotatedTarget "ATM sets the transaction to initiated if the withdrawal sequence was successful."^^xsd:string .

atm:InvalidBankCodeTransaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Message ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Class ;
                        owl:complementOf atm:ValidBankCodeTransaction ] ) ] .

atm:InvalidPasswordTransaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Message ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:InvalidPasswordAuthorization ] ) ] .

atm:RestartWhenAmountExceedsPolicy a owl:Axiom ;
    owl:annotatedProperty rdfs:comment ;
    owl:annotatedSource atm:Transaction ;
    owl:annotatedTarget "Handle transaction restart when amount exceeds transaction policy."^^xsd:string .

atm:SuccessfulAuthorizationTriggersDispensing a owl:Axiom ;
    owl:annotatedProperty rdfs:comment ;
    owl:annotatedSource atm:Authorization ;
    owl:annotatedTarget "Successful authorization triggers money dispensing."^^xsd:string .

atm:UnsuccessfulTransaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:Message ] ) ] .

atm:BankCodeValidAuthorization a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( atm:CashCard [ a owl:Restriction ;
                                owl:onProperty atm:bankCode ;
                                owl:someValuesFrom xsd:string ] ) ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:authorizedBy ],
        atm:Authorization .

atm:InvalidPasswordAuthorization a owl:Class ;
    rdfs:subClassOf atm:Authorization .

atm:ValidBankCodeTransaction a owl:Class ;
    rdfs:subClassOf atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:BankCodeValidAuthorization ] ) ] .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ] .

atm:messageCode a owl:DatatypeProperty ;
    rdfs:domain atm:Message ;
    rdfs:range xsd:string .

atm:serialNumber a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:bankCode a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:BankComputer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:ATM ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:ATM ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:ATM ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:ATM ] .

atm:Message a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ] .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ] .

atm:Authorization a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ] .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ] .

