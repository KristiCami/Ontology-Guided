@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

atm:ATM_dispenses_Transaction a owl:Axiom ;
    atm:sourceRequirement "FR-14" .

atm:ATM_logs_CashCard a owl:Axiom ;
    atm:sourceRequirement "FR-6" .

atm:ATM_logs_RequirementTarget a owl:Axiom ;
    atm:sourceRequirement "FR-15" .

atm:ATM_relatesTo_CashCard a owl:Axiom ;
    atm:sourceRequirement "FR-2",
        "FR-3",
        "FR-4" .

atm:ATM_relatesTo_RequirementTarget a owl:Axiom ;
    atm:sourceRequirement "FR-1",
        "FR-11",
        "FR-9" .

atm:ATM_relatesTo_Transaction a owl:Axiom ;
    atm:sourceRequirement "FR-12",
        "FR-13",
        "FR-16" .

atm:Bank_logs_RequirementTarget a owl:Axiom ;
    atm:sourceRequirement "FR-8" .

atm:Bank_relatesTo_CashCard a owl:Axiom ;
    atm:sourceRequirement "FR-1",
        "FR-10",
        "FR-3",
        "FR-5" .

atm:Bank_relatesTo_RequirementTarget a owl:Axiom ;
    atm:sourceRequirement "FR-2",
        "FR-4",
        "FR-7" .

atm:dispenses a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

atm:logs a owl:ObjectProperty ;
    rdfs:domain atm:ATM,
        atm:Bank ;
    rdfs:range atm:CashCard,
        atm:RequirementTarget .

atm:relatesTo a owl:ObjectProperty ;
    rdfs:domain atm:ATM,
        atm:Bank ;
    rdfs:range atm:CashCard,
        atm:RequirementTarget,
        atm:Transaction .

atm:Bank a owl:Class .

atm:CashCard a owl:Class .

atm:RequirementTarget a owl:Class .

atm:Transaction a owl:Class .

atm:ATM a owl:Class .

