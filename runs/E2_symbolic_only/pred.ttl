@prefix atm: <http://example.org/atm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:AmountExceedsTransactionPolicy rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:triggers ;
            owl:someValuesFrom atm:TransactionRestart ] .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( atm:BankComputer atm:BankSoftware ) ] ;
            owl:onProperty atm:providesSecurityFor ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass atm:ATM ;
            owl:onProperty atm:processesTransactionsFrom ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasLimitK ;
            owl:someValuesFrom xsd:decimal ] .

atm:FR5Situation a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:ValidCashCard atm:ValidPassword atm:AccountProblem [ a owl:Restriction ;
                        owl:onProperty atm:triggers ;
                        owl:someValuesFrom atm:BankSendsMessageToATM ] ) ] .

atm:FR6Situation a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:ValidCashCard atm:ValidPassword [ a owl:Class ;
                        owl:complementOf atm:AccountProblem ] [ a owl:Restriction ;
                        owl:onProperty atm:triggers ;
                        owl:someValuesFrom atm:BankComputerSendsOkMessageToATM ] ) ] .

atm:FR7Situation a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Request [ a owl:Restriction ;
                        owl:onProperty atm:followedBy ;
                        owl:someValuesFrom atm:BankComputerProcessesTransaction ] ) ] .

atm:FR8Situation a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:MoneyDispensed [ a owl:Restriction ;
                        owl:onProperty atm:followedBy ;
                        owl:someValuesFrom atm:AccountUpdated ] ) ] .

atm:FunctionalRequirement1 a atm:FunctionalRequirement,
        owl:NamedIndividual ;
    atm:hasText "If ATM is initialized with t dollars and parameters k,m,n are entered, then parameters are set."^^xsd:string ;
    atm:hasTitle "Functional requirement 1"^^xsd:string .

atm:FunctionalRequirement16 a atm:FunctionalRequirement,
        owl:NamedIndividual .

atm:FunctionalRequirement2 a atm:FunctionalRequirement,
        owl:NamedIndividual ;
    atm:hasText "If no cash card is in the ATM, the system should display initial display."^^xsd:string ;
    atm:hasTitle "Functional requirement 2"^^xsd:string .

atm:FunctionalRequirement3 a atm:FunctionalRequirement,
        owl:NamedIndividual ;
    atm:hasText "If the ATM is running out of money, no card should be accepted. An error message is displayed."^^xsd:string ;
    atm:hasTitle "Functional requirement 3"^^xsd:string .

atm:FunctionalRequirement4 a atm:FunctionalRequirement,
        owl:NamedIndividual ;
    atm:hasText "The ATM checks if the entered card is valid. If not, it displays an error message and returns the card."^^xsd:string ;
    atm:hasTitle "Functional requirement 4"^^xsd:string .

atm:FunctionalRequirement5 a atm:FunctionalRequirement,
        owl:NamedIndividual ;
    atm:hasText "If the cash card is valid, the ATM should read the serial number and bank code."^^xsd:string ;
    atm:hasTitle "Functional requirement 5"^^xsd:string .

atm:SuccessfulAuthorization rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:triggers ;
            owl:someValuesFrom atm:MoneyDispensing ] .

atm:SuccessfulTransaction rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:triggers ;
            owl:someValuesFrom atm:MoneyDispensing ] .

atm:SuccessfulWithdrawalSequence rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue atm:Initiated ;
            owl:onProperty atm:setsTransactionTo ] .

atm:System rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:errorMessageDuration ;
            owl:someValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:decimal ;
                    owl:withRestrictions ( [ xsd:minInclusive 30.0 ] ) ] ] .

atm:ATM rdfs:subClassOf [ a owl:Class ;
            owl:equivalentClass [ a owl:Class ;
                    owl:intersectionOf ( [ a owl:Restriction ;
                                owl:hasValue true ;
                                owl:onProperty atm:dispensesMoney ] [ a owl:Restriction ;
                                owl:hasValue true ;
                                owl:onProperty atm:withdrawalProcessedAndAccepted ] ) ] ] .

atm:BankComputer rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:responseTimeout ;
            owl:someValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:decimal ;
                    owl:withRestrictions ( [ xsd:maxInclusive 120.0 ] ) ] ] .

atm:MoneyDispensing rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:triggers ;
            owl:someValuesFrom atm:AmountLogging ] .

