@prefix atm: <http://example.org/atm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

atm:ATMInitializedWithTDollarsAndParametersEntered a owl:Class ;
    rdfs:subClassOf atm:ParametersSet ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:ATMInitialized atm:InitializedWithTDollars atm:ParametersEntered ) ] .

atm:ATMRunningOutOfMoney a owl:Class ;
    rdfs:subClassOf atm:DisplaysErrorMessage,
        atm:NoCardAccepted .

atm:AmountExceedsTransactionPolicy rdfs:subClassOf atm:TransactionRestart .

atm:CashCardValid a owl:Class ;
    rdfs:subClassOf atm:ReadsBankCode,
        atm:ReadsSerialNumber .

atm:EnteredCardInvalid a owl:Class ;
    rdfs:subClassOf atm:DisplaysErrorMessage,
        atm:ReturnsCard .

atm:FunctionalRequirement10 a atm:FunctionalRequirement,
        owl:NamedIndividual .

atm:FunctionalRequirement6 a atm:FunctionalRequirement,
        owl:NamedIndividual .

atm:FunctionalRequirement7 a atm:FunctionalRequirement,
        owl:NamedIndividual .

atm:FunctionalRequirement8 a atm:FunctionalRequirement,
        owl:NamedIndividual .

atm:FunctionalRequirement9 a atm:FunctionalRequirement,
        owl:NamedIndividual .

atm:NoCashCardInATM a owl:Class ;
    rdfs:subClassOf atm:DisplaysInitialDisplay .

atm:SuccessfulAuthorization rdfs:subClassOf atm:MoneyDispensing .

atm:SuccessfulTransaction rdfs:subClassOf atm:MoneyDispensing .

atm:SuccessfulWithdrawalSequence rdfs:subClassOf atm:TransactionInitiated .

atm:MoneyDispensing rdfs:subClassOf atm:AmountLogged .

