@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:BankCodeValidCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:CashCard [ a owl:Restriction ;
                        owl:onProperty atm:bankCode ;
                        owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                        owl:onProperty atm:issuesCard ;
                        owl:someValuesFrom atm:Bank ] ) ] .

atm:CardEjection a owl:Class ;
    rdfs:subClassOf atm:Transaction .

atm:ErrorMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:InvalidBankCodeMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:InvalidPasswordMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ] .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Bank ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Bank ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( [ a owl:Class ;
                                owl:complementOf atm:TransactionNotSuccessful ] [ a owl:Class ;
                                owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                                            owl:onProperty atm:sendsMessageTo ;
                                            owl:someValuesFrom atm:BankComputer ] ) ] ) ] ;
            owl:onProperty atm:handlesTransaction ] .

atm:TransactionNotSuccessful a owl:Class ;
    rdfs:subClassOf atm:Transaction .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:maintainsAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ] .

atm:Customer rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:hasAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:ownsCard ] .

atm:Message a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:messageCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:messageCode ] .

atm:Authorization a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty atm:transactionTimestamp ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:requestedAmount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ] .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:bankCode ] .

atm:BankComputer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:ATM ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:ATM ;
            owl:onProperty atm:sendsMessageTo ] .

[] a owl:Class .

[] a owl:Restriction ;
    owl:onProperty atm:bankCode ;
    owl:someValuesFrom xsd:string .

[] a owl:Restriction .

