@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:Deposit a owl:Class ;
    rdfs:subClassOf atm:Transaction .

atm:TransactionNotSuccessful a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Message ] ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:unionOf ( atm:InvalidBankCodeAuthorization atm:InvalidPasswordAuthorization ) ] ] ) ] .

atm:ValidBankCodeAuthorization a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Authorization [ a owl:Restriction ;
                        owl:onProperty atm:usesCard ;
                        owl:someValuesFrom [ a owl:Restriction ;
                                owl:onProperty atm:bankCode ;
                                owl:someValuesFrom xsd:string ] ] ) ] .

atm:accepts a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:CashCard .

atm:condition a owl:ObjectProperty ;
    rdfs:domain atm:ComplexRequirement ;
    rdfs:range atm:BasicRequirement .

atm:consistOfKeys a owl:ObjectProperty ;
    rdfs:domain atm:Keypad ;
    rdfs:range atm:Keys .

atm:dispenses a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Money .

atm:displays a owl:Class ;
    rdfs:subClassOf atm:ATM .

atm:else a owl:ObjectProperty ;
    rdfs:domain atm:ComplexRequirement ;
    rdfs:range atm:BasicRequirement .

atm:fromBankComputer a owl:Class ;
    rdfs:subClassOf atm:BankComputer .

atm:hasBackUpPowerSupply a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:PowerSupply .

atm:hasCardVerificationTime a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Time .

atm:hasCashCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:hasMainPowerSupply a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:PowerSupply .

atm:hasPinVerificationTime a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Time .

atm:subject a owl:ObjectProperty ;
    rdfs:domain atm:BasicRequirement ;
    rdfs:range atm:Subject .

atm:types a owl:Class ;
    rdfs:subClassOf atm:Customer .

atm:updates a owl:Class ;
    rdfs:subClassOf atm:ATMtotalFundOfATM .

atm:verb a owl:ObjectProperty ;
    rdfs:domain atm:BasicRequirement ;
    rdfs:range atm:Verb .

atm:ATMtotalFundOfATM a owl:Class .

atm:InvalidBankCodeAuthorization a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Authorization [ a owl:Restriction ;
                        owl:onProperty atm:usesCard ;
                        owl:someValuesFrom [ a owl:Restriction ;
                                owl:onProperty atm:bankCode ;
                                owl:someValuesFrom xsd:string ] ] ) ] .

atm:InvalidPasswordAuthorization a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Authorization [ a owl:Restriction ;
                        owl:onProperty atm:usesCard ;
                        owl:someValuesFrom atm:CashCard ] ) ] .

atm:Keypad a owl:Class .

atm:Keys a owl:Class .

atm:Money a owl:Class .

atm:Subject a owl:Class .

atm:Verb a owl:Class .

atm:transactionTimestamp a owl:Class ;
    rdfs:subClassOf atm:Transaction .

atm:ComplexRequirement a owl:Class .

atm:Message a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ] .

atm:PowerSupply a owl:Class .

atm:Time a owl:Class .

atm:handlesTransaction a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

atm:hasAccount a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:Account .

atm:maintainsAccount a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:Account .

atm:operatedBy a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Bank .

atm:ownsCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:requestedAmount a owl:DatatypeProperty ;
    rdfs:label "Generated from failed competency question" ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:decimal .

atm:serialNumber a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:transactionTimestamp ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Withdrawal [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:Authorization ] [ a owl:Restriction ;
                        owl:onProperty atm:dispensedAmount ;
                        owl:someValuesFrom xsd:decimal ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( atm:Withdrawal [ a owl:Restriction ;
                        owl:onProperty atm:dispensedAmount ;
                        owl:someValuesFrom xsd:decimal ] [ a owl:Restriction ;
                        owl:onProperty atm:requestedAmount ;
                        owl:someValuesFrom xsd:decimal ] ) ] .

atm:messageCode a owl:DatatypeProperty ;
    rdfs:domain atm:Message ;
    rdfs:range xsd:string .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:maintainsAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:issuesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ] .

atm:BasicRequirement a owl:Class .

atm:bankCode a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:dispensedAmount a owl:DatatypeProperty ;
    rdfs:label "Generated from failed competency question" ;
    rdfs:domain atm:Withdrawal ;
    rdfs:range xsd:decimal .

atm:Customer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:ownsCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:ownsCard ;
            owl:someValuesFrom atm:CashCard ] .

atm:BankComputer a owl:Class .

atm:Account a owl:Class .

atm:Authorization rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ] .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ] .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:operatedBy ;
            owl:someValuesFrom atm:Bank ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:operatedBy ;
            owl:someValuesFrom atm:Bank ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ] .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ] .

