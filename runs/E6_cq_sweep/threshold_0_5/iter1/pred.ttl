        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:messageCode ],
        [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ] .

atm:MoneyDispensedAmountLogged a owl:Axiom ;
    rdfs:comment "Functional requirement 15: If the money is dispensed, the amount is logged."@en .

atm:SuccessfulAuthorizationTriggersDispensing a owl:Axiom ;
    rdfs:comment "Functional requirement 11: Successful authorization triggers money dispensing."@en .

atm:TransactionSucceedsATMDispensesMoney a owl:Axiom ;
    rdfs:comment "Functional requirement 14: If the transaction succeeds, the ATM dispenses money."@en .

atm:Withdrawal rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
