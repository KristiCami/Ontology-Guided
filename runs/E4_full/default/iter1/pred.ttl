@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:Deposit rdfs:subClassOf atm:Transaction .

atm:InvalidBankCodeCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:CashCard [ a owl:Class ;
                        owl:complementOf atm:ValidBankCodeCard ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( atm:CashCard [ a owl:Class ;
                        owl:complementOf atm:ValidBankCodeCard ] ) ] .

atm:InvalidPasswordCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:CashCard [ a owl:Class ;
                        owl:complementOf atm:ValidPasswordCard ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( atm:CashCard [ a owl:Class ;
                        owl:complementOf atm:ValidPasswordCard ] ) ] .

atm:displays rdfs:subClassOf atm:ATM .

atm:fromBankComputer rdfs:subClassOf atm:BankComputer .

atm:hasCashCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:types rdfs:subClassOf atm:Customer .

atm:updates rdfs:subClassOf atm:ATMtotalFundOfATM .

atm:Keypad rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:consistOfKeys ;
            owl:someValuesFrom atm:Keys ] .

atm:accepts a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:CashCard .

atm:condition a owl:ObjectProperty ;
    rdfs:domain atm:ComplexRequirement ;
    rdfs:range atm:BasicRequirement .

atm:consistOfKeys a owl:ObjectProperty ;
    rdfs:domain atm:Keypad ;
    rdfs:range atm:Keys .

atm:dispenses a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Money .

atm:else a owl:ObjectProperty ;
    rdfs:domain atm:ComplexRequirement ;
    rdfs:range atm:BasicRequirement .

atm:hasBackUpPowerSupply a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:PowerSupply .

atm:hasCardVerificationTime a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Time .

atm:hasMainPowerSupply a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:PowerSupply .

atm:hasPinVerificationTime a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Time .

atm:subject a owl:ObjectProperty ;
    rdfs:domain atm:BasicRequirement ;
    rdfs:range atm:Subject .

atm:verb a owl:ObjectProperty ;
    rdfs:domain atm:BasicRequirement ;
    rdfs:range atm:Verb .

atm:ComplexRequirement rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:condition ;
            owl:someValuesFrom atm:BasicRequirement ],
        [ a owl:Restriction ;
            owl:onProperty atm:else ;
            owl:someValuesFrom atm:BasicRequirement ] .

atm:ErrorMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:ValidBankCodeCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty atm:issuedBy ;
            owl:someValuesFrom atm:Bank ],
        [ a owl:Restriction ;
            owl:onProperty atm:issuedBy ;
            owl:someValuesFrom atm:Bank ] .

atm:ValidPasswordCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty atm:hasPassword ;
            owl:someValuesFrom atm:Password ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasPassword ;
            owl:someValuesFrom atm:Password ] .

atm:displaysMessage a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Message .

atm:ejectsCard a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:CashCard .

atm:hasAccount a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:Account .

atm:hasPassword a owl:ObjectProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range atm:Password .

atm:issuedBy a owl:ObjectProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range atm:Bank ;
    owl:inverseOf atm:issuesCard .

atm:maintainsAccount a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:Account .

atm:operatedBy a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Bank .

atm:ownsCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:transactionTimestamp rdfs:subClassOf atm:Transaction .

atm:Password a owl:Class .

atm:bankCode rdfs:subClassOf atm:bankCode .

atm:TransactionNotSuccessful a owl:Class ;
    rdfs:subClassOf atm:Transaction .

atm:requestedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:decimal .

atm:Withdrawal rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Withdrawal [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:Authorization ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( atm:Withdrawal [ a owl:Restriction ;
                        owl:onProperty atm:dispensedAmount ;
                        owl:someValuesFrom xsd:decimal ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( atm:Withdrawal [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:Authorization ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( atm:Withdrawal [ a owl:Restriction ;
                        owl:onProperty atm:dispensedAmount ;
                        owl:someValuesFrom xsd:decimal ] ) ] .

atm:BasicRequirement rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:subject ;
            owl:someValuesFrom atm:Subject ],
        [ a owl:Restriction ;
            owl:onProperty atm:verb ;
            owl:someValuesFrom atm:Verb ] .

atm:Customer rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:ownsCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:hasAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:ownsCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:hasAccount ] .

atm:handlesTransaction a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

atm:Bank rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ] .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:requestedAmount ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty atm:transactionTimestamp ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:requestedAmount ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty atm:transactionTimestamp ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ] .

atm:dispensedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Withdrawal ;
    rdfs:range xsd:decimal .

atm:BankComputer rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:ATM ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty owl:topObjectProperty ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:ATM ] .

atm:Authorization a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                                owl:onProperty atm:usesCard ;
                                owl:someValuesFrom atm:CashCard ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                                owl:onProperty atm:usesCard ;
                                owl:someValuesFrom atm:CashCard ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:displaysMessage ;
            owl:someValuesFrom atm:ErrorMessage ],
        [ a owl:Restriction ;
            owl:onProperty atm:ejectsCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Bank ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                                owl:onProperty atm:usesCard ;
                                owl:someValuesFrom atm:CashCard ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                                owl:onProperty atm:usesCard ;
                                owl:someValuesFrom atm:CashCard ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:displaysMessage ;
            owl:someValuesFrom atm:ErrorMessage ],
        [ a owl:Restriction ;
            owl:onProperty atm:ejectsCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:accepts ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispenses ;
            owl:someValuesFrom atm:Money ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasBackUpPowerSupply ;
            owl:someValuesFrom atm:PowerSupply ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasCardVerificationTime ;
            owl:someValuesFrom atm:Time ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasMainPowerSupply ;
            owl:someValuesFrom atm:PowerSupply ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasPinVerificationTime ;
            owl:someValuesFrom atm:Time ],
        [ a owl:Restriction ;
            owl:onProperty atm:operatedBy ;
            owl:someValuesFrom atm:Bank ] .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ] .

