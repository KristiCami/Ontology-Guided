@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:BalanceInquiry a owl:Class .

atm:Deposit a owl:Class .

atm:FR11_SuccessfulAuthorizationTriggersDispensing a owl:Axiom ;
    rdfs:comment "Successful authorization triggers money dispensing."@en .

atm:FR12_RestartWhenAmountExceedsPolicy a owl:Axiom ;
    rdfs:comment "Handle transaction restart when amount exceeds transaction policy."@en .

atm:FR13_SetTransactionInitiatedIfWithdrawalSequenceSuccessful a owl:Axiom ;
    rdfs:comment "ATM sets the transaction to initiated if the withdrawal sequence was successful."@en .

atm:FR14_IfTransactionSucceedsATMDispensesMoney a owl:Axiom ;
    rdfs:comment "If the transaction succeeds, the ATM dispenses money."@en .

atm:FR15_IfMoneyDispensedAmountLogged a owl:Axiom ;
    rdfs:comment "If the money is dispensed, the amount is logged."@en .

atm:InvalidBankCodeCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ],
        atm:CashCard ;
    owl:disjointWith atm:ValidBankCodeCard ;
    owl:equivalentClass [ a owl:Class ;
            owl:complementOf atm:ValidBankCodeCard ] .

atm:InvalidPasswordCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        atm:CashCard ;
    owl:disjointWith atm:ValidPasswordCard .

atm:ErrorMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:SuccessfulTransaction a owl:Class ;
    rdfs:subClassOf atm:Transaction ;
    owl:disjointWith atm:UnsuccessfulTransaction .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:Authorization ] ) ] .

atm:cardEjectedAfter a owl:ObjectProperty ;
    rdfs:domain atm:UnsuccessfulTransaction ;
    rdfs:range atm:CashCard .

atm:displaysMessage a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Message .

atm:hasAccount a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:Account .

atm:hasBankCodeValidityCheck a owl:ObjectProperty ;
    rdfs:domain atm:BankComputer ;
    rdfs:range atm:CashCard .

atm:hasPasswordValidityCheck a owl:ObjectProperty ;
    rdfs:domain atm:BankComputer ;
    rdfs:range atm:CashCard .

atm:issuedBy a owl:ObjectProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range atm:Bank ;
    owl:inverseOf atm:issuesCard .

atm:messageCode a owl:DatatypeProperty ;
    rdfs:domain atm:Message ;
    rdfs:range xsd:string .

atm:onAccount a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Account .

atm:ownsCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:performedBy a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Customer .

atm:requestedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:decimal .

atm:transactionTimestamp a owl:DatatypeProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:dateTime .

atm:ValidPasswordCard a owl:Class ;
    rdfs:subClassOf atm:CashCard .

atm:bankCode a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:dispensedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Withdrawal ;
    rdfs:range xsd:decimal .

atm:serialNumber a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:UnsuccessfulTransaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:cardEjectedAfter ;
            owl:someValuesFrom atm:CashCard ],
        atm:Transaction .

atm:ValidBankCodeCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty atm:issuedBy ;
            owl:someValuesFrom atm:Bank ] .

atm:authorizedBy a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Authorization .

atm:handlesTransaction a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

atm:issuesCard a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:CashCard .

atm:operatedBy a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Bank .

atm:Authorization a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:Customer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:ownsCard ;
            owl:someValuesFrom atm:CashCard ] .

atm:Message a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ] .

atm:maintainsAccount a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:Account .

atm:receivesMessageFrom a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:BankComputer .

atm:sendsMessageTo a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:BankComputer .

atm:usesCard a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:CashCard .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:maintainsAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:issuesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Restriction ;
                    owl:hasSelf true ;
                    owl:onProperty atm:maintainsAccount ] ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ] .

atm:Account a owl:Class .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:operatedBy ;
            owl:someValuesFrom atm:Bank ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:displaysMessage ;
            owl:someValuesFrom atm:ErrorMessage ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( atm:SuccessfulTransaction atm:UnsuccessfulTransaction ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( atm:Transaction ) ] ;
            owl:onProperty atm:handlesTransaction ] .

atm:BankComputer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:ATM ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:ATM ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:ATM ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:ValidBankCodeCard ;
            owl:onProperty atm:hasBankCodeValidityCheck ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:ValidPasswordCard ;
            owl:onProperty atm:hasPasswordValidityCheck ] .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:transactionTimestamp ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ] .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ] .

