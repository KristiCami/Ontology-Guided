@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:InvalidBankCodeMessage a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:sentFromBankComputer ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sentToATM ;
            owl:someValuesFrom atm:ATM ],
        atm:Message .

atm:InvalidPasswordMessage a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:sentFromBankComputer ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sentToATM ;
            owl:someValuesFrom atm:ATM ],
        atm:Message .

atm:ValidBankCodeCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty atm:issuedBy ;
            owl:someValuesFrom atm:Bank ] .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:transactionTimestamp ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:dispensedAmount ],
        atm:Transaction .

atm:displaysMessage a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Message .

atm:ejectsCard a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:CashCard .

atm:issuedBy a owl:ObjectProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range atm:Bank ;
    owl:inverseOf atm:issuesCard .

atm:ErrorMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:hasMessage a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Message .

atm:sentFromBankComputer a owl:ObjectProperty ;
    rdfs:domain atm:Message ;
    rdfs:range atm:BankComputer .

atm:sentToATM a owl:ObjectProperty ;
    rdfs:domain atm:Message ;
    rdfs:range atm:ATM .

atm:TransactionNotSuccessful a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasMessage ;
            owl:someValuesFrom atm:ErrorMessage ],
        atm:Transaction .

atm:hasCard a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:CashCard .

atm:Customer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:hasAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:ownsCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:hasAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:ownsCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:hasAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:ownsCard ] .

atm:TransactionNotSuccessfulWithCard a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                        owl:onProperty atm:hasCard ;
                        owl:someValuesFrom atm:CashCard ] ) ] .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( [ a owl:Class ;
                                owl:complementOf atm:TransactionNotSuccessfulWithCard ] [ a owl:Class ;
                                owl:intersectionOf ( atm:TransactionNotSuccessfulWithCard [ a owl:Restriction ;
                                            owl:onProperty atm:hasMessage ;
                                            owl:someValuesFrom atm:ErrorMessage ] ) ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( [ a owl:Class ;
                                owl:complementOf atm:TransactionNotSuccessfulWithCard ] [ a owl:Class ;
                                owl:intersectionOf ( atm:TransactionNotSuccessfulWithCard [ a owl:Restriction ;
                                            owl:onProperty atm:hasCard ;
                                            owl:someValuesFrom atm:CashCard ] ) ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( [ a owl:Class ;
                                owl:complementOf atm:TransactionNotSuccessful ] [ a owl:Class ;
                                owl:intersectionOf ( atm:TransactionNotSuccessful [ a owl:Restriction ;
                                            owl:onProperty atm:hasCard ;
                                            owl:someValuesFrom atm:CashCard ] ) ] ) ] ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Bank ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:operatedBy ;
            owl:someValuesFrom atm:Bank ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Bank ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Bank ;
            owl:onProperty atm:operatedBy ] .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:maintainsAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:issuesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ] .

atm:Authorization a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ] .

atm:Message a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:messageCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:messageCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:messageCode ] .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty atm:transactionTimestamp ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:requestedAmount ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:requestedAmount ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty atm:transactionTimestamp ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty atm:requestedAmount ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty atm:transactionTimestamp ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Customer ;
            owl:onProperty atm:performedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:onAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:usesCard ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Authorization ;
            owl:onProperty atm:authorizedBy ] .

atm:BankComputer rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:ATM ;
            owl:onProperty atm:sendsMessageTo ] .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:bankCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:bankCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty atm:bankCode ] .

