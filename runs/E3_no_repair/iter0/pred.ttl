@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:ATMThatSendsMessageToBankComputer a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:ATM [ a owl:Restriction ;
                        owl:onProperty atm:sendsMessageTo ;
                        owl:someValuesFrom atm:BankComputer ] ) ] .

atm:BankComputerSendsMessageToATM a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:ATM [ a owl:Restriction ;
                        owl:onProperty atm:receivesMessageFrom ;
                        owl:someValuesFrom atm:BankComputer ] ) ] .

atm:ErrorMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:InvalidBankCodeMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:InvalidPasswordMessage a owl:Class ;
    rdfs:subClassOf atm:Message .

atm:UnsuccessfulTransaction a owl:Class ;
    rdfs:subClassOf atm:Transaction .

atm:ValidBankCodeCard a owl:Class ;
    rdfs:subClassOf atm:CashCard ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( [ a owl:Restriction ;
                        owl:onProperty atm:bankCode ;
                        owl:someValuesFrom xsd:string ] [ a owl:Restriction ;
                        owl:onProperty atm:serialNumber ;
                        owl:someValuesFrom xsd:string ] ) ] .

atm:hasAccount a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:Account .

atm:ownsCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:messageCode a owl:DatatypeProperty ;
    rdfs:domain atm:Message ;
    rdfs:range xsd:string .

atm:onAccount a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Account .

atm:performedBy a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Customer .

atm:requestedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:decimal .

atm:transactionTimestamp a owl:DatatypeProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:dateTime .

atm:usesCard a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:CashCard .

atm:bankCode a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:dispensedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Withdrawal ;
    rdfs:range xsd:decimal .

atm:issuesCard a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:CashCard .

atm:maintainsAccount a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:Account .

atm:serialNumber a owl:DatatypeProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:BalanceInquiry a owl:Class ;
    rdfs:subClassOf atm:Transaction ;
    owl:disjointWith atm:Bank,
        atm:BankComputer,
        atm:CashCard,
        atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:handlesTransaction a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

atm:sendsMessageTo a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:BankComputer .

atm:authorizedBy a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Authorization .

atm:receivesMessageFrom a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:BankComputer .

atm:Authorization a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Transaction ] ;
    owl:disjointWith atm:BalanceInquiry,
        atm:Bank,
        atm:BankComputer,
        atm:CashCard,
        atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:Account a owl:Class ;
    owl:disjointWith atm:Authorization,
        atm:BalanceInquiry,
        atm:Bank,
        atm:BankComputer,
        atm:CashCard,
        atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:ATM a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Transaction ;
            owl:onProperty atm:handlesTransaction ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:BankComputer ;
            owl:onProperty atm:sendsMessageTo ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ],
        [ a owl:Restriction ;
            owl:onProperty atm:receivesMessageFrom ;
            owl:someValuesFrom atm:BankComputer ],
        [ a owl:Restriction ;
            owl:onProperty atm:handlesTransaction ;
            owl:someValuesFrom atm:Transaction ] ;
    owl:disjointWith atm:Account,
        atm:Authorization,
        atm:BalanceInquiry,
        atm:Bank,
        atm:BankComputer,
        atm:CashCard,
        atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:Bank a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:inverseOf atm:operatedBy ;
            owl:onProperty atm:operatedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:Account ;
            owl:onProperty atm:maintainsAccount ],
        [ a owl:Restriction ;
            owl:allValuesFrom atm:CashCard ;
            owl:onProperty atm:issuesCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:issuesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:maintainsAccount ;
            owl:someValuesFrom atm:Account ] ;
    owl:disjointWith atm:BankComputer,
        atm:CashCard,
        atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:Deposit a owl:Class ;
    rdfs:subClassOf atm:Transaction ;
    owl:disjointWith atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:Customer a owl:Class ;
    owl:disjointWith atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:BankComputer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( atm:ATM [ a owl:Restriction ;
                                owl:onProperty atm:operatedBy ;
                                owl:someValuesFrom atm:Bank ] ) ] ;
            owl:onProperty atm:receivesMessageFrom ],
        [ a owl:Restriction ;
            owl:onProperty atm:sendsMessageTo ;
            owl:someValuesFrom atm:ATM ] ;
    owl:disjointWith atm:CashCard,
        atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        atm:Transaction ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( atm:Transaction [ a owl:Restriction ;
                        owl:onProperty atm:authorizedBy ;
                        owl:someValuesFrom atm:Authorization ] ) ] .

atm:Message a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:messageCode ;
            owl:someValuesFrom xsd:string ] ;
    owl:disjointWith atm:Transaction,
        atm:Withdrawal .

atm:CashCard a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:bankCode ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty atm:serialNumber ;
            owl:someValuesFrom xsd:string ] ;
    owl:disjointWith atm:Customer,
        atm:Deposit,
        atm:Message,
        atm:Transaction,
        atm:Withdrawal .

atm:operatedBy a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Bank .

atm:Transaction a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:transactionTimestamp ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:authorizedBy ;
            owl:someValuesFrom atm:Authorization ] ;
    owl:disjointWith atm:Withdrawal .

