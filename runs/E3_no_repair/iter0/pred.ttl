@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm:Bank_1 a atm:Bank ;
    atm:issuesCard atm:CashCard_1 ;
    atm:maintainsAccount atm:Account_1 .

atm:ATM_1 a atm:ATM ;
    atm:handlesTransaction atm:Transaction_1 ;
    atm:performedBy atm:Customer_1 ;
    atm:sendsMessageTo atm:BankComputer_1 ;
    atm:transactionTimestamp "2023-10-01T12:00:00"^^xsd:dateTime .

atm:CashCard_1 a atm:CashCard ;
    atm:bankCode "XYZ123"^^xsd:string ;
    atm:serialNumber "123456789"^^xsd:string .

atm:Customer a atm:Customer ;
    atm:hasAccount atm:Account ;
    atm:ownsCard atm:CashCard ;
    atm:performedBy atm:Transaction .

atm:Transaction_1 a atm:Transaction ;
    atm:authorizedBy atm:Authorization_1 ;
    atm:messageCode "Transaction NOT successful"^^xsd:string ;
    atm:receivesMessageFrom atm:BankComputer_1 ;
    atm:transactionTimestamp "2023-10-01T12:00:00"^^xsd:dateTime .

atm:ATM a atm:ATM ;
    atm:contactsBank "Environment:bank" ;
    atm:displaysMessage "Item:error message",
        "Item:message" ;
    atm:ejectsCard "Item:card" ;
    atm:handlesTransaction atm:Transaction,
        atm:Withdrawal ;
    atm:issuesCard atm:Bank ;
    atm:keepsCard "Item:card" ;
    atm:operatedBy atm:Bank,
        atm:BankComputer ;
    atm:receivesMessageFrom atm:BankComputer ;
    atm:sendsMessageTo atm:BankComputer ;
    atm:startsTransactionDialog "Item:transaction dialog" ;
    atm:verifiesBankCode "Item:bank code"^^xsd:string ;
    atm:verifiesPassword "Item:password"^^xsd:string .

atm:BankComputer_1 a atm:BankComputer ;
    atm:messageCode "bad bank code"^^xsd:string,
        "password is invalid"^^xsd:string ;
    atm:receivesMessageFrom atm:ATM_1 .

atm:CashCard a atm:CashCard ;
    atm:bankCode "bank_code"^^xsd:string ;
    atm:logs "Item:log file" ;
    atm:serialNumber "Item:Serial number"^^xsd:string,
        "serial_number"^^xsd:string .

atm:Transaction a atm:Transaction ;
    atm:authorizedBy atm:Authorization ;
    atm:requestedAmount "t"^^xsd:decimal .

