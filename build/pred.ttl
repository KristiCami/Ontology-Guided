@prefix atm: <http://example.org/atm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

atm:ATMTransactionError a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty atm:resultsIn ;
            owl:someValuesFrom atm:ErrorMessage ] .

atm:Accepted a owl:Class .

atm:AuthorizationProcess a owl:Class .

atm:AuthorizationSuccessful a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasFunction ;
            owl:someValuesFrom atm:FunctionAuthorization ] .

atm:BadAccount a owl:Class .

atm:BadBankCode a owl:Class .

atm:BadPassword a owl:Class .

atm:BadPasswordMessage a owl:Class ;
    rdfs:subClassOf atm:FlowMessage .

atm:BankCode a owl:Class ;
    owl:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:Verify ;
            owl:someValuesFrom atm:BankComputer ] .

atm:BankCodeInvalid a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasBankCode ;
            owl:someValuesFrom atm:StateInvalid ] .

atm:BankCodeValid a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasBankCode ;
            owl:someValuesFrom atm:StateValid ] .

atm:CardEjected a owl:Class ;
    rdfs:subClassOf atm:FunctionEject .

atm:CardVerificationRequest a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:requestsVerification ;
            owl:someValuesFrom atm:Card ] .

atm:Customer a atm:Actor,
        owl:Class ;
    atm:displaysErrorMessage atm:Function ;
    atm:entersCard atm:cash_card ;
    atm:returnsCard atm:cash_card ;
    atm:setsCardValidity atm:cash_card ;
    owl:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:Contact ;
            owl:someValuesFrom atm:Bank ] .

atm:CustomerEnteredMoney a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasActor ;
            owl:someValuesFrom atm:ActorCustomer ] .

atm:CustomerTakesCard a owl:ObjectProperty ;
    rdfs:domain atm:ActorCustomer ;
    rdfs:range atm:ItemCard .

atm:DispensedTwentyDollarBills a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasFunction ;
            owl:someValuesFrom atm:FunctionDispense ] .

atm:InvalidBankCodeMessage a owl:Class ;
    rdfs:subClassOf atm:FlowMessage .

atm:InvalidPassword a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasPassword ;
            owl:someValuesFrom atm:StateInvalid ] .

atm:Message a owl:Class .

atm:MoneyDispensed a owl:Class ;
    rdfs:subClassOf atm:FunctionDispensed .

atm:MoneyToBeDispensed a owl:Class ;
    rdfs:subClassOf atm:StateValueToBeDispensed .

atm:PasswordVerificationRequest a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:requestsVerification ;
            owl:someValuesFrom atm:Password ] .

atm:Reject a owl:ObjectProperty .

atm:SerialNumberFromCashCard a owl:Class ;
    owl:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:Log ;
            owl:someValuesFrom atm:SerialNumber ] .

atm:TransactionInitiated a owl:Class ;
    rdfs:subClassOf atm:StateValueInitiated .

atm:TransactionLimitExceeded a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:exceedsItem ;
            owl:someValuesFrom atm:ItemTransactionLimit ] .

atm:TransactionNotSuccessful a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasStateValue ;
            owl:someValuesFrom atm:StateValueNotSuccessful ] .

atm:TransactionSucceeded a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasFlowMessage ;
            owl:someValuesFrom atm:FlowMessage ] .

atm:User a owl:Class ;
    owl:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:Verify ;
            owl:someValuesFrom atm:Password ] .

atm:ValidPassword a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasPassword ;
            owl:someValuesFrom atm:StateValid ] .

atm:WithdrawalSequenceSuccessful a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:hasFunction ;
            owl:someValuesFrom atm:FunctionWithdrawal ] .

atm:WrongPassword a owl:Class .

atm:cancelTransaction a owl:ObjectProperty ;
    rdfs:domain atm:FunctionCancel ;
    rdfs:range atm:ItemTransaction .

atm:dispenseMoney a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:ItemMoney .

atm:logAmount a owl:ObjectProperty ;
    rdfs:domain atm:FunctionLog ;
    rdfs:range atm:ItemSerialNumber .

atm:restartTransactionDialog a owl:ObjectProperty ;
    rdfs:domain atm:FunctionTransactionDialog ;
    rdfs:range atm:FunctionRestart .

atm:sendResponseToBank a owl:ObjectProperty ;
    rdfs:domain atm:EnvironmentBank ;
    rdfs:range atm:ItemMoney .

atm:waitForResponse a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:SystemBankComputer .

atm:withdraw a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:FunctionWithdraw .

atm:Accept a owl:ObjectProperty .

atm:Authorization a owl:Class .

atm:Bank a owl:Class .

atm:BankComputer a owl:Class .

atm:Contact a owl:ObjectProperty .

atm:Display a owl:ObjectProperty .

atm:Eject a owl:ObjectProperty .

atm:Log a owl:ObjectProperty .

atm:LogFile a owl:Class ;
    owl:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:Update ;
            owl:someValuesFrom atm:LogFile ] .

atm:Start a owl:ObjectProperty .

atm:TransactionDialog a owl:Class .

atm:Update a owl:ObjectProperty .

atm:ErrorMessage a owl:Class ;
    rdfs:subClassOf atm:FunctionDisplay .

atm:Password a owl:Class .

atm:Verify a owl:ObjectProperty .

atm:ATM a atm:System,
        owl:Class ;
    atm:displaysErrorMessage atm:Function ;
    atm:doesNotAcceptCard atm:card ;
    atm:hasParameter atm:k,
        atm:m,
        atm:n ;
    atm:initializedWith atm:t ;
    atm:initializesParameters atm:k,
        atm:m,
        atm:n,
        atm:t ;
    atm:noCashCard atm:cash_card ;
    atm:parametersSet atm:State ;
    atm:runningOutOfMoney atm:State ;
    atm:setsDisplay atm:initialDisplay ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:setsItem ;
            owl:someValuesFrom atm:ItemMoney ] ;
    owl:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:Accept ;
            owl:someValuesFrom atm:Authorization ],
        [ a owl:Restriction ;
            owl:onProperty atm:Eject ;
            owl:someValuesFrom atm:Card ],
        [ a owl:Restriction ;
            owl:onProperty atm:Display ;
            owl:someValuesFrom atm:ErrorMessage ],
        [ a owl:Restriction ;
            owl:onProperty atm:Start ;
            owl:someValuesFrom atm:TransactionDialog ],
        [ a owl:Restriction ;
            owl:onProperty atm:Keep ;
            owl:someValuesFrom atm:Card ] .

atm:cash_card a atm:Item ;
    atm:initiatesAuthorizationDialog atm:Function ;
    atm:isValid atm:StateValue ;
    atm:sendsBankCode atm:bank_code ;
    atm:sendsSerialNumber atm:serial_number .

