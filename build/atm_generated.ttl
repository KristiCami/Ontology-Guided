@prefix atm: <http://lod.csd.auth.gr/atm/atm.ttl#> .
@prefix atm1: <http://example.org/atm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

atm1:ATM a atm1:System ;
    atm1:displaysInitialDisplay atm1:display ;
    atm1:hasParameter atm1:k,
        atm1:m,
        atm1:n ;
    atm1:initializeParameters atm1:k,
        atm1:m,
        atm1:n,
        atm1:t ;
    atm1:initializedWith atm1:t ;
    atm1:noCashCardIn atm1:cashCard ;
    atm1:parametersSet atm1:State .

atm:BalanceInquiry a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        atm:Transaction .

atm:Deposit a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        atm:Transaction .

atm:authorizedBy a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Authorization .

atm:handlesTransaction a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Transaction .

atm:hasAccount a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:Account .

atm:issuesCard a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:CashCard .

atm:maintainsAccount a owl:ObjectProperty ;
    rdfs:domain atm:Bank ;
    rdfs:range atm:Account .

atm:messageCode a owl:DatatypeProperty ;
    rdfs:domain atm:Message ;
    rdfs:range xsd:string .

atm:operatedBy a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:Bank .

atm:ownsCard a owl:ObjectProperty ;
    rdfs:domain atm:Customer ;
    rdfs:range atm:CashCard .

atm:receivesMessageFrom a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:BankComputer .

atm:sendsMessageTo a owl:ObjectProperty ;
    rdfs:domain atm:ATM ;
    rdfs:range atm:BankComputer .

atm:transactionTimestamp a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:dateTime .

atm:Authorization a owl:Class .

atm:Message a owl:Class .

atm:Withdrawal a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty atm:performedBy ;
            owl:someValuesFrom atm:Customer ],
        [ a owl:Restriction ;
            owl:onProperty atm:onAccount ;
            owl:someValuesFrom atm:Account ],
        [ a owl:Restriction ;
            owl:onProperty atm:usesCard ;
            owl:someValuesFrom atm:CashCard ],
        [ a owl:Restriction ;
            owl:onProperty atm:requestedAmount ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty atm:dispensedAmount ;
            owl:someValuesFrom xsd:decimal ],
        atm:Transaction .

atm:bankCode a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:dispensedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Withdrawal ;
    rdfs:range xsd:decimal .

atm:serialNumber a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain atm:CashCard ;
    rdfs:range xsd:string .

atm:BankComputer a owl:Class .

atm:requestedAmount a owl:DatatypeProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range xsd:decimal .

atm:Bank a owl:Class .

atm:onAccount a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Account .

atm:performedBy a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:Customer .

atm:usesCard a owl:ObjectProperty ;
    rdfs:domain atm:Transaction ;
    rdfs:range atm:CashCard .

atm:ATM a owl:Class .

atm:Account a owl:Class .

atm:Customer a owl:Class .

atm:CashCard a owl:Class ;
    owl:hasKey ( atm:bankCode atm:serialNumber ) .

atm:Transaction a owl:Class .

